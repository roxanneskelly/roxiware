  <setup>
    <![CDATA[person_id = Roxiware::Param::Param.application_param_val("people", "default_biography")
person = Roxiware::Person.find(person_id)

author_name =  @current_layout.get_by_path("_/header_bar/logo_bar/display_items/item-0/text")
author_name.value = person.full_name
author_name.save!

Roxiware::Param::Param.set_application_param("system", "webmaster_email", "9041D4FC-D97F-44F0-BFF2-FC2B4D3F6270", person.email)
Roxiware::Param::Param.set_application_param("system", "title", "0B8CFD7C-39AA-4E61-A4F2-9E7212EF9415", person.full_name+", Author")
Roxiware::Param::Param.set_application_param("system", "site_copyright", "4454EFC0-EC3C-4C9D-8EF5-6F848E3B33D7", person.full_name)
Roxiware::Param::Param.set_application_param("system", "meta_description", "F0E1D8A9-33B9-4605-B10F-831D2BB3D423", person.full_name)
Roxiware::Param::Param.set_application_param("system", "meta_keywords", "1843B11F-EBA1-4B9A-A01F-F98A3E458FA8", person.full_name+", Author")
Roxiware::Param::Param.set_application_param("blog", "blog_editor_email", "89139210-E699-4D47-A656-B2F860D2015B", person.email)
Roxiware::Param::Param.set_application_param("blog", "blog_title", "3D102055-8C9B-4838-90F7-2B3C7DB23A1D", person.full_name+", Author")]]>
  </setup>
  <categories>
  </categories>
  <style>
    <![CDATA[/* Document layout */
/* NOTE: Be careful when adding styles to the application body.  Management controls, dialogs and such are contained within.
    It's better to apply styles to the #mainBody which only contains client website content */
body.application_body
{
  background: <%= background %>;
}


/* Fonts and text */
div#mainBody
{
    font-size:12px;
    font-family:Geneva, Verdana, Arial, Helvetica, sans-serif;

    padding-left: 10px;
    padding-right: 10px;
    margin:0px auto 20px auto;
    background: <%= background %>;
    color:<%= text_color %>;

    h1, h2, h3, h4, h5, h6 {
	font-family:Georgia, serif;
	color:<%= text_color %>;
    }

    h1 {font-size:1.3em;line-height:1;margin-bottom:0.5em;}
    h2 {font-size:1.2em;margin-bottom:0.75em;}
    h3 {font-size:1.1em;line-height:1;margin-bottom:1em;}
    h4 {font-size:1.1em;line-height:1.25;margin-bottom:1.25em;}
    h5 {font-size:1em;font-weight:bold;margin-bottom:1.5em;}
    h6 {font-size:1em;font-weight:bold;}

    hr {
        color: <%= text_color %>;
        background: <%= text_color %>;
    }
    b {
        text-shadow:2px 2px 4px rgba(0, 0, 0, 0.7);
    }
   
    a, a:visited, a:link {
        color: <%= link_color %>;
        text-decoration:none;
        font-style:none;
    }

    a, a:visited, a:link, a:hover {
        color: <%= link_color %>;
    }
    a:hover {
      color: <%= link_hover_color %>;
    }
    
  .watermark {
       color:#ccc !important;
    }
    input {
        margin:0px;
        padding:1px;
    }
    label {
        padding:2px
    }

    button {
        border:none;
        background:<%= text_color %>;
        color:<%= background %>;
        border-radius:0px;
        padding:0.2em 3em 0.2em 3em;
    }
    button:hover {
        background:<%= link_color %>;
    }
}


/* General layout */
div#mainBody
{

    header {
       width:100%;
    }

    #layout_body
    {
       margin:0px;
       width:100%;
       display:block;
      
    }

    #content_section {
       display:block;
       width:100%;
    }

    #center_bar {
        width:100%;
    }

    #left_bar_section, #right_bar_section, #center_bar
    {
       position:relative;
       display:inline-block;
       vertical-align:top;
       margin:0px;
    }

    #center_content
    {
       position:relative;
       width:100%;
    }
}


/* Sizing for various devices */
div#mainBody {
     width:600px;
     font-size:12px;
}

/* for all mobile devices, set width to 100% */
@media all and (max-device-width: 1024px) {
      div#mainBody {
          width:100%;
      }
}

/* Various mobile portrait guesses */
@media all and (min-device-height: 320px) and (orientation:portrait) {
      div#mainBody {
          min-height:300px;
           font-size:8px;
      }
}

@media all and (min-device-height: 480px) and (orientation:portrait) {
      div#mainBody {
          min-height:450px;
      }
}

@media all and (min-device-height: 640px) and (orientation:portrait) {
      div#mainBody {
          min-height:600px;
      }
}

@media all and (device-height: 800px) and (orientation:portrait) {
      div#mainBody {
          min-height:750px;
           font-size:9px;
      }
}

@media all and (min-device-height: 1024px) and (orientation:portrait) {
      div#mainBody {
          min-height:940px;
           font-size:11px;
      }
}

@media all and (min-device-height: 1280px) and (orientation:portrait) {
      div#mainBody {
          min-height:1200px;
          font-size:12px;
      }
}

/* Landscape */
@media all and (min-device-width: 320px) and (orientation:landscape) {
      div#mainBody {
          min-height:300px;
          font-size:8px;
      }
}

@media all and (min-device-width: 480px) and (orientation:landscape) {
      div#mainBody {
          min-height:450px;
      }
}

@media all and (min-device-width: 640px) and (orientation:landscape) {
      div#mainBody {
          min-height:600px;
          font-size:9px;
      }
}

@media all and (min-device-width: 768px) and (orientation:landscape) {
      div#mainBody {
          min-height:680px;
      }
}

@media all and (min-device-width: 800px) and (orientation:landscape) {
      div#mainBody {
          min-height:750px;
          font-size:10px;
      }
}

@media all and (min-device-width: 1024px) and (orientation:landscape) {
      div#mainBody {
          min-height:940px;
          font-size:11px;
      }
}

@media all and (device-width: 1280px) and (device-height:800px) and (orientation:landscape) {
      div#mainBody {
          min-height:1200px;
          font-size:12px;
      }
}


/* Content and widget background boxes */
/* set the content background to relative so we can position the content background off of it */
div.content_background_container, .widget
{
   position:relative;
}

/* The backgrounds for the content and widget containers span the entire widget */
div.widget_background, div.content_background
{
    position:absolute;
    width:100%;
    height:100%;
    z-index:-1;
}


/* Post*/
div#mainBody .post {
    position:relative;
    margin: 0;
    width:100%;
    overflow:hidden;
    
    div.share_bar {
        position:absolute;
        top:0.5em;
        right:1em;
        > div {
            display:inline-block;
            vertical-align:middle;
            width:90px;
            height:30px;
        }
    }

    hr.post_separator {
         margin:0 10% 0 10%;
         width:80%;
    }

    .post_categories {
        display:none;
    }

    .post_title {
        position:absolute;
        top:1.0em;
        font-size:3em;
        font-family:Times New Roman,Georgia,Serif;
    }

    /* Various mobile portrait guesses */
    @media all and (min-device-height: 320px) and (orientation:portrait) {
        .post_title {
            font-size:1.5em;
            margin-top:1em;
        }
    }

    .post_header {
        position:absolute;
        top:0.5em;
        width:90%;
        left:5%;
        color: <%= link_color %>;
 
        .post_author_img {
            display:none;
        }
        
        .post_date {
            display:inline-block;
         }

         .post_author {
            display:inline-block;
         }
         .post_author:before {
             content:"by ";
         }

         .comments_link {
            display:inline-block;
         }
         .comments_link:before {
             content: "\00a0\00a0|\00a0\00a0";
         }
    }
    .content {
        margin-top:6em;
        margin-left:0em;
        overflow:hidden;
        width:100%;
    }

}

.comments {
    margin-top:2em;
    .comment {

        border-top:solid 1px <%= text_color %>;
        position:relative;
        clear:both;
        margin-bottom:3em;
        .comment {
            margin-left:3em;
         }

        .comment_status_moderate {
            float:right;
            color: <%= text_alert_color %>;
            margin-right:4em;
        }
        .comment_status_publish {
              display:none;
        }

        .comment_header {
            vertical-align:top;
            color: <%= link_color %>;
           

            .comment_author_image {
               display:none;
            }

            .comment_author_name {
                display:inline-block;
                margin-right:0.5em;
                float:left;
            }
            .comment_author_date:before {
                content:"on ";
            }
            .comment_author_date {
                display:inline-block;
                margin-right:1em;
            }
        }
        .comment_footer {
            clear:both;
            text-align:center;
            .comment_reply {
                cursor:pointer;
                
            }
        }
    }
    .comment:hover {
        .manage_button {
            cursor:pointer;
            background:url('<%= CGI::unescape cdn_icon("settings_16", "clean", active_asset_color_scheme.to_s) %>');
        }
    }
}

.post:hover {
    .manage_button {
        cursor:pointer;
        background:url('<%= CGI::unescape cdn_icon("settings_16", "clean", active_asset_color_scheme.to_s) %>');
    }
}

/* Full post display with comments */
.post_listing {
    .post_footer {
        hr.post_separator {
            margin:0 10% 0 10%;
            width:80%;
        }
        .comment_count {
            font-size:1.3em;
            display:inline-block;
        }
        .pending_comment_count {
            display:inline-block;
            color: <%= text_alert_color %>;
            font-size:1.3em;
            margin-left:2em;
        }

        .comment_form {
            position:relative;
             width:100%;
            margin:2em 0 3em 0;

            label {
                width:5em;
            }
            div.comment_form_header {
                position:relative;
                width:100%;
                height:10em;
                input.comment_author, div.comment_author {
                    position:absolute;
                    top:0px;
                    left:80px;
                    right:20%;
                }
                div.comment_author {
                   font-size:1.5em;
                }
                label.comment_author {
                    position:absolute;
                    left:2em;
                    top:0px;
                }
                .comment_author_image {
                    display:none;
                } 
                textarea.comment_form_content {
                    resize:none;
                    position:absolute;
                    right:0px;
                    left:80px;
                    top:2em;
                    height:6em;   
                }
             }
             button.post_comment_button {
                    float:right;
             }
            .comment_form_footer {
                position:relative;
                width:100%;
                .comment_auth_info {
                    position:relative;
                    height:80px;
                }
                label {
                   width:5em !important;
                }
                label.comment_form_author_url {
                    position:absolute;
                    left:0px;
                    top:0px;
                } 
                input.comment_form_author_url {
                    position:absolute;
                    top:0px;
                    left:80px;
                    right:330px;
                }
                label.comment_form_author_email {
                    position:absolute;
                    top:2em;
                    left:0px;
                }
                input.comment_form_author_email {
                    position:absolute;
                    top:2em;
                    left:80px;
                    right:330px;
                }
                 .recaptcha_response_field {
                      position:absolute;
                      top:4em;
                      left:80px;
                      right:330px;
                  }

                .recaptcha_image {
                    width:300px;
                    height:57px;
                    position:absolute;
                    top:0px;
                    right:20px;
                    border:solid 1px black;
                }
                .recaptcha_only_if_incorrect_sol {
                     position:absolute;
                     top:60px;
                     right:1em;
                 }
                  .captcha_reload, .captcha_audio, .captcha_image, .captcha_help {
                      position:absolute;
                      right:0px;            
                      width:16px;
                      height:16px;
                      cursor:pointer;
                  }
                  .captcha_reload {
                        top:0px;
                        background:url('<%= CGI::unescape cdn_icon("twitter_16", "clean", asset_color_scheme.to_s) %>') no-repeat center;
                   }
                   .captcha_audio {
                        top:16px;
                        background:url('<%= CGI::unescape cdn_icon("twitter_16", "clean", asset_color_scheme.to_s) %>') no-repeat center;
                    }
                    .captcha_image {
                        top:16px;
                        background:url('<%= CGI::unescape cdn_icon("twitter_16", "clean", asset_color_scheme.to_s) %>') no-repeat center;
                    }
                    .captcha_help {
                        top:32px;
                        background:url('<%= CGI::unescape cdn_icon("twitter_16", "clean", asset_color_scheme.to_s) %>') no-repeat center;
                    }
                
            }
        }
    }
}

.page_content, .person_listing {
    .manage_button {
        position:absolute;
        top:0px;
        right:0px;
        background:url('<%= CGI::unescape cdn_icon("settings_16", "clean", active_asset_color_scheme.to_s) %>');
    }

    .manage_button:hover {
        background:url('<%= CGI::unescape cdn_icon("settings_16", "clean", asset_color_scheme.to_s) %>');
    }
}


.series_listing {

    clear:both;
    position:relative;
    border-top:solid 1px <%= text_color %>;
    padding:1em;

   margin:1em 0 2em 0;
   width:100%;
   a.series_link {
      div.series_title {
           font-size:1.5em;
       }
   }

   div.series_books {
       margin:1em 0em 0em 3em;
       .series_book_titles {
           .series_book_title {
               display:block;
           }
       }
   }
}


div.book_listing {
    clear:both;
    position:relative;
    border-top:solid 1px <%= text_color %>;
    padding:1em;
    a {
        .book_title {
             font-size:1.3em;
             margin:0 0 1em 0;
        }
        img {
            float:left;
            margin:1em 2em 2em 0;
        }
     }
}


.bookstore_list {
     float:right;
         ul {
          list-style-type:none;
         li.book_store {
              display:inline-block;
              a.store_link {
                  width:32px;
                  height:32px;
                  display:inline-block;
                  div.store_name {
                      display:none;
                  }
              }
             <% %w(sony amazon kobo barnsandnoble).each do |store| %>
                 a.store_link_<%= store %>{
                     background:url('<%= CGI::unescape cdn_icon("#{store}_32", "clean", asset_color_scheme.to_s) %>') no-repeat center;
                } 
            <% end %>   
             <% %w(sony amazon kobo barnsandnoble).each do |store| %>
                 a.store_link_<%= store %>:hover{
                     background:url('<%= CGI::unescape cdn_icon("#{store}_32", "clean", active_asset_color_scheme.to_s) %>') no-repeat center;
                } 
            <% end %>   
       }
    }
}

.person_listing {
    .social_network_icons {
        display:none;
    }

    .person_portrait {
        float:right;
        margin:1em 0 2em 2em;
        padding:0.5em;
        border:solid 1px black;

    }
    .person_name {
        font-size:2em;
    }
}


/* WIDGETS */
.widget {
    width:100%;
    margin:0px;

    .widget_content {
        margin:0px;
        padding:0px;
        position:relative;

        .widget_content h1 {
            font-size:1.2em;
            font-weight:700;
        }
    }
}

/* Individual widget settings */
#header_bar_section {
    position:relative;
    height:100px;
    margin:0 0 3em 0;

    .widget#logo_bar {
        position:absolute;
        left:0px;
        top:0px;
        width:20%;
        height:80px;
    }

    .widget#social_networks_header {
         position:absolute;
         right:0px;
         top:2em;
         width:60%;
         .widget_content {
             text-align:right;
             .social_networks_icon {
                 padding:0 5px 0 5px;
             }
         }
    }

    .widget#login_bar {
        position:absolute;
        right:0px;
        top:0px;
        height:2em;
        padding:0.2em;
        width:100%;
        .widget_content {
           text-align:right;
       }
    }

    /* Main Menu layout */
    .widget#main_menu { 
        position:absolute;
        left:0px;
        bottom:0px;
        width:65%;
        .ddsmoothmenu_widget {
            font: Verdana;
            margin-left:auto;
            margin-right:auto;

            display:inline-block;
            height:1.5em;
            ul {
                list-style-type:none;
                display:inline-block;
                li { 
                   display:inline-block;
                   vertical-align:middle;
                   text-align:center;
                   padding:0px;

                   a, a:link, a:visited {
                       color: <%= text_color %>;
	               background: none;
                       display:inline-block;
                       padding:0 0.5em 0 0.5em;
                       text-decoration:none;
                       width:auto;
                       font-size:1.2em;
                       line-height:1.5em;
                   }

	           a.menu_selected,
	           ul li a.menu_selected {
  	               color: <%= link_color %>;
	           }

                   a:hover, a.selected, ul li a:hover   {
	               color: <%= link_color %>
                   }
       
                   ul {
                      padding-right:0px;
                      position: absolute;
                      left:0;
                      display: none;
                      visibility: hidden;
                      width: auto;
                      li {
                          width:100%;
                          padding-bottom:0px;
                          display: list-item;
                          margin:0;
                          height:auto;
                          border:none;
                          a {
                              padding-top:0.5em;
                              padding-bottom:0.5em;
                              margin:0em;
                          }
                      }
                  }
              }
          }
      }
  }
}
/* Social networking icons */
<% %w(youtube goodreads google facebook twitter rss).each do |soc_net| %>
a#<%= soc_net %>.social_networks_icon {
     display:inline-block;
     background:url('<%= CGI::unescape cdn_icon("#{soc_net}_32", "clean", asset_color_scheme.to_s) %>') no-repeat center;
     width:32px;
     height:32px;
}    
a#<%= soc_net %>.social_networks_icon:hover {
     display:inline-block;
     background:url('<%= CGI::unescape cdn_icon("#{soc_net}_32", "clean", active_asset_color_scheme.to_s) %>') no-repeat center;
     width:32px;
     height:32px;
}    
<% end %>
<% %w(google facebook twitter).each do |soc_net| %>
a.<%= soc_net %>_button {
     display:inline-block;
     background:url('<%= CGI::unescape cdn_icon("#{soc_net}_16", "clean", asset_color_scheme.to_s) %>') no-repeat center;
     width:16px;
     height:16px;
}    
a.<%= soc_net %>_button:hover {
     display:inline-block;
     background:url('<%= CGI::unescape cdn_icon("#{soc_net}_16", "clean", active_asset_color_scheme.to_s) %>') no-repeat center;
     width:16px;
     height:16px;
}    
<% end %>

/* Prev/Next post */
.widget#prev_next_post {
    text-align:center;
    margin:2em 0em 1em 0em;
    .widget_content {
        width:20em;
        .prev_post {
            float:left;
        }
        .prev_post:before {
           content: "<< ";
        }

        .next_post {
             float:right;
        }
        .next_post:after {
            content:" >>";
        }
    }


}]]>
  </style>
  <params>
    <param description="0ADB2780-3D21-409F-A7B4-670395F99CD5" class="local" name="series_display_type">simple</param>
    <param description="AE10A07E-03BF-4583-BCA0-B2550786E5F4" class="local" name="home_page_type">blog_post</param>
    <param description="1FA46E6A-3DB1-4B32-ABC3-532A66ABC0C0" class="local" name="books_display_type">simple</param>
    <param description="1EFF4D33-DCDF-4245-BC49-90D1C6F1FABA" class="local" name="series_book_description_length">200</param>
    <param description="CDC341F5-9EFB-41DB-8B84-B1101A58704D" class="local" name="post_date_format">%e %b %Y</param>
    <param description="1EFF4D33-DCDF-4245-BC49-90D1C6F1FABA" class="local" name="book_excerpt_length">1000</param>
    <param description="67BFE233-F4D1-400B-855F-393BF53B05A4" class="local" name="asset_theme">clear</param>
    <param description="CDC341F5-9EFB-41DB-8B84-B1101A58704D" class="local" name="event_date_format">%b %e</param>
    <param description="1EFF4D33-DCDF-4245-BC49-90D1C6F1FABA" class="local" name="book_description_length">500</param>
